class Solution {
    public String firstPalindrome(String[] words) {
       StringBuilder str = new StringBuilder();
        for(int i=0;i<words.length;i++){
    str.append(words[i]);
    str.reverse();
    if(str.toString().equals (words[i]))
    return words[i];
str.setLength(0);



        }

return "";
    }
}
